[
  {
    "uuid": "flow1",
    "name": "Registration Flow",
    "language": "eng",
    "nodes": [
      {
        "uuid": "node1",
        "actions": [
          {
            "type": "reply",
            "uuid": "9d9290a7-3713-4c22-8821-4af0a64c0821",
            "text": "Hi! What is your name?"
          }
        ],
        "exits": [
          {
            "uuid": "node1-exit1",
            "destination_node_uuid": "node2"
          }
        ]
      },
      {
        "uuid": "node2",
        "wait": {
          "type": "msg",
          "timeout": 600
        },
        "router": {
          "type": "switch",
          "default_exit_uuid": "node2-exit2",
          "name": "name",
          "operand": "@input",
          "cases": [
            {
              "uuid": "node2-case1",
              "type": "has_text",
              "exit_uuid": "node2-exit1"
            }
          ]
        },
        "exits": [
          {
            "uuid": "node2-exit1",
            "label": "Not Empty",
            "destination_node_uuid": "node3"
          },
          {
            "uuid": "node2-exit2",
            "label": "Other",
            "destination_node_uuid": "node2"
          }
        ]
      },
      {
        "uuid": "node3",
        "actions": [
          {
            "uuid": "455ba297-f6d2-45e6-bf3e-c1ef028b55ae",
            "type": "update_contact",
            "field_name": "name",
            "value": "@input.text"
          },
          {
            "uuid": "b3fa763e-474b-49df-b4d6-15e86507668f",
            "type": "add_to_group",
            "groups": [
              {
                "uuid": "7be2f40b-38a0-4b06-9e6d-522dca592cc8",
                "name": "Registered Users"
              }
            ]
          },
          {
            "uuid": "605e3486-503d-481c-94f7-cd553f196a8a",
            "type": "reply",
            "text": "Great, you are @contact.name, thanks for joining!"
          }
        ]
      }
    ]
  }
]